<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.PasswordField?>

<VBox fx:id="vRoot" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AccountController">
   <children>
      <Label fx:id="title" alignment="CENTER" prefWidth="500.0" text="Create your account.">
         <VBox.margin>
            <Insets top="60.0" />
         </VBox.margin>
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Label alignment="CENTER" prefWidth="500.0" text="Username">
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
         <font>
            <Font size="18.0" />
         </font></Label>
      <TextField fx:id="username" alignment="CENTER" maxWidth="200.0" prefHeight="27.0" prefWidth="200.0">
         <VBox.margin>
            <Insets left="150.0" top="5.0" />
         </VBox.margin>
      </TextField>
      <Label alignment="CENTER" prefWidth="500.0" text="Password">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </Label>
      <PasswordField fx:id="password" alignment="CENTER" maxWidth="200.0" prefHeight="27.0" prefWidth="200.0">
         <VBox.margin>
            <Insets left="150.0" top="5.0" />
         </VBox.margin>
      </PasswordField>
      <Label fx:id="error" alignment="CENTER" prefHeight="17.0" prefWidth="547.0" text="Error Line">
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="create" maxWidth="100.0" mnemonicParsing="false" prefWidth="0.0" text="Create!">
         <VBox.margin>
            <Insets left="200.0" top="60.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
